<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Estudiante" table="estudiante">
    <indexes>
      <index name="fk_tipoIdentificacionEst" columns="tipoIdentificacion"/>
      <index name="fk_programa" columns="programa"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="codigo" columns="codigo"/>
      <unique-constraint name="identificacion" columns="identificacion"/>
    </unique-constraints>
    <id name="idestudiante" type="integer" column="idEstudiante">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codigo" type="string" column="codigo" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="identificacion" type="string" column="identificacion" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombres" type="string" column="nombres" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidos" type="string" column="apellidos" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="tipoidentificacion" target-entity="Tipoidentificacion" fetch="LAZY">
      <join-columns>
        <join-column name="tipoIdentificacion" referenced-column-name="idTipoIdentificacion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="programa" target-entity="Programa" fetch="LAZY">
      <join-columns>
        <join-column name="programa" referenced-column-name="idPrograma"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
