<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Entrenamiento" table="entrenamiento">
    <indexes>
      <index name="fk_idClasificacion" columns="idClasificacion"/>
      <index name="fk_idCaracteristica" columns="idCaracteristica"/>
    </indexes>
    <id name="identrenamiento" type="integer" column="idEntrenamiento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="valor" type="integer" column="valor" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idclasificacion" target-entity="Clasificacion" fetch="LAZY">
      <join-columns>
        <join-column name="idClasificacion" referenced-column-name="idClasificacion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idcaracteristica" target-entity="Caracteristica" fetch="LAZY">
      <join-columns>
        <join-column name="idCaracteristica" referenced-column-name="idCaracteristica"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
