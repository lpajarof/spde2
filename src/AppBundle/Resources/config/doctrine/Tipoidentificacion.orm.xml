<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tipoidentificacion" table="tipoidentificacion">
    <id name="idtipoidentificacion" type="integer" column="idTipoIdentificacion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipo" type="string" column="tipo" length="4" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="string" column="descripcion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
