<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tipousuario" table="tipousuario">
    <unique-constraints>
      <unique-constraint name="tipoUsuario" columns="tipoUsuario"/>
      <unique-constraint name="descripcion" columns="descripcion"/>
    </unique-constraints>
    <id name="idtipousuario" type="integer" column="idTipoUsuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipousuario" type="integer" column="tipoUsuario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="descripcion" type="string" column="descripcion" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
