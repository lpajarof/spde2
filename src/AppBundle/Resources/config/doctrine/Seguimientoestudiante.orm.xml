<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Seguimientoestudiante" table="seguimientoestudiante">
    <indexes>
      <index name="fk_idEstudianteSeg" columns="idEstudiante"/>
      <index name="fk_idAccion" columns="idAccion"/>
      <index name="fk_asignadoA" columns="asignadoA"/>
    </indexes>
    <id name="idseguimientoestudiante" type="integer" column="idSeguimientoEstudiante">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="observaciones" type="string" column="observaciones" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechainicio" type="datetime" column="fechaInicio" nullable="false"/>
    <field name="fechafin" type="string" column="fechaFin" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idestudiante" target-entity="Estudiante" fetch="LAZY">
      <join-columns>
        <join-column name="idEstudiante" referenced-column-name="idEstudiante"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idaccion" target-entity="Accion" fetch="LAZY">
      <join-columns>
        <join-column name="idAccion" referenced-column-name="idAccion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="asignadoa" target-entity="Usuario" fetch="LAZY">
      <join-columns>
        <join-column name="asignadoA" referenced-column-name="idUsuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
