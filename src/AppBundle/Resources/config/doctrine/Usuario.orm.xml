<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuario" table="usuario">
    <indexes>
      <index name="fk_tipoIdentificacion" columns="idTipoIdentificacion"/>
      <index name="fk_tipousuario" columns="idTipoUsuario"/>
    </indexes>
    <id name="idusuario" type="integer" column="idUsuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="identificacion" type="string" column="identificacion" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombres" type="string" column="nombres" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidos" type="string" column="apellidos" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contrasena" type="string" column="contrasena" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idtipousuario" target-entity="Tipousuario" fetch="LAZY">
      <join-columns>
        <join-column name="idTipoUsuario" referenced-column-name="idTipoUsuario"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idtipoidentificacion" target-entity="Tipoidentificacion" fetch="LAZY">
      <join-columns>
        <join-column name="idTipoIdentificacion" referenced-column-name="idTipoIdentificacion"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
